{
  "version": 2,
  "builds": [
    {
      "src": "src/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/src/qa-automation-multi-store.js"
    },
    {
      "src": "/auth/callback",
      "dest": "/src/qa-automation-multi-store.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "/src/qa-automation-multi-store.js"
    },
    {
      "src": "/(.*)",
      "dest": "/src/qa-automation-multi-store.js"
    }
  ]
}
